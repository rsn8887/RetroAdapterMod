  67:usbdrv/usbdrvasm16.inc ****     inc     YL
  68:usbdrv/usbdrvasm16.inc ****     sts     usbSofCount, YL
  69:usbdrv/usbdrvasm16.inc **** #endif  /* USB_COUNT_SOF */
